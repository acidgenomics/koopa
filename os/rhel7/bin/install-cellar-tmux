#!/usr/bin/env bash

script_dir="$(cd "$(dirname "${BASH_SOURCE[0]}")" >/dev/null 2>&1 && pwd -P)"
# shellcheck source=/dev/null
source "${script_dir}/../include/header.sh"

if _koopa_has_sudo
then
    sudo yum install -y tmux
    sudo yum-builddep -y tmux
fi

# shellcheck source=/dev/null
source "$(_koopa_cellar_script tmux)"
