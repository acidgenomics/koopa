#!/usr/bin/env bash
set -Eeu -o pipefail

# shellcheck source=/dev/null
source "$(koopa header darwin)"



# Usage                                                                     {{{1
# ==============================================================================

usage() {
cat << EOF
$(_koopa_help_header)
    name

Open application.

required positional arguments with defaults:
    1.  Application name.

$(_koopa_help_args)

note:
    Bash script.
    Updated 2019-06-23.
EOF
}

_koopa_help "$@"



# Arguments                                                                 {{{1
# ==============================================================================

name="$1"



# Script                                                                    {{{1
# ==============================================================================

open -a "${name}.app"
