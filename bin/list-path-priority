#!/usr/bin/env bash

KOOPA_HOME="$(cd "$(dirname "${BASH_SOURCE[0]}")/.." \
    >/dev/null 2>&1 && pwd -P)"
# shellcheck source=/dev/null
source "${KOOPA_HOME}/shell/bash/include/header.sh"



# Notes                                                                     {{{1
# ==============================================================================




# Usage                                                                     {{{1
# ==============================================================================

usage() {
cat << EOF
$(_koopa_help_header)

List PATH priority.

$(_koopa_help_args)

details:
    Splits the PATH string by ':' delim.

note:
    Bash script.
    Updated 2019-10-27.
EOF
}

_koopa_help "$@"
_koopa_assert_has_no_args "$@"



# Script                                                                    {{{1
# ==============================================================================

_koopa_list_path_priority
