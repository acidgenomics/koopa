#!/usr/bin/env bash
set -Eeu -o pipefail

# shellcheck source=/dev/null
source "$(koopa header bash)"

url="ftp://ftp.ncbi.nlm.nih.gov/refseq/release/RELEASE_NUMBER"



# Usage                                                                     {{{1
# ==============================================================================

usage() {
cat << EOF
$(_koopa_help_header)

RefSeq version.

$(_koopa_help_args)

details:
    Extract the RefSeq release version from ftp.ncbi.nlm.nih.gov.

see also:
    ${url}

note:
    Bash script.
    Updated 2019-09-24.
EOF
}

_koopa_help "$@"



# Script                                                                    {{{1
# ==============================================================================

wget -q -O - "$url"
