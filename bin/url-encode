#!/usr/bin/env python3
"""
URL encode.
Updated 2020-08-12.
"""

import sys
from koopa.url import url_encode

# FIXME Need to add support for --help flag.
# FIXME Can we source a shared Python header that detects --help flag?

def main(x):
    """
    URL encode.
    """
    print(url_encode(x))


main(sys.argv[1])
