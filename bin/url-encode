#!/usr/bin/env python3
"""
URL encode.
Updated 2020-08-14.
"""

import sys

from koopa.strings import url_encode
from koopa.system import koopa_help

koopa_help()


# FIXME How to parameterize?
# FIXME Need to error on unset positional arguments.

def main(x):
    """
    URL encode.
    """
    print(url_encode(x))


main(sys.argv[1])
