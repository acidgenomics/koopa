#!/usr/bin/env bash
set -Eeu -o pipefail

# shellcheck source=/dev/null
source "$(koopa header bash)"

# Usage                                                                     {{{1
# ==============================================================================

usage() {
cat << EOF
usage: r-javareconf [--help|-h]

Reconfigure rJava.

optional arguments:
    --help, -h
        Show this help message and exit.

note:
    Bash script.
    Updated 2019-06-24.
EOF
}

# Script                                                                    {{{1
# ==============================================================================

_koopa_r_javareconf
