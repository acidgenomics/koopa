#!/usr/bin/env bash
set -Eeu -o pipefail

# Install spacemacs.
# Modified 2019-06-18.

if [[ -d ".emacs.d" ]]
then
    >&2 printf "Error: '.emacs.d/' directory exists.\n"
    exit 1
fi

git clone git@github.com:syl20bnr/spacemacs.git ~/.emacs.d

emacs

# Fix for org-projectile warning.
# > rm -f ~/.emacs.d/elpa/org-projectile-*/org-projectile.elc
