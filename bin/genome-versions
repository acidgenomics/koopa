#!/usr/bin/env bash
# shellcheck disable=SC2001

KOOPA_HOME="$(cd "$(dirname "${BASH_SOURCE[0]}")/.." \
    >/dev/null 2>&1 && pwd -P)"
# shellcheck source=/dev/null
source "${KOOPA_HOME}/shell/bash/include/header.sh"



# Usage                                                                     {{{1
# ==============================================================================

usage() {
cat << EOF
$(_koopa_help_header)

Genome versions.

$(_koopa_help_args)

note:
    Bash script.
    Updated 2019-09-22.
EOF
}

_koopa_help "$@"



# Script                                                                    {{{1
# ==============================================================================

ensembl="$(ensembl-version)"
gencode="$(gencode-version "Homo sapiens")"
refseq="$(refseq-version)"
flybase="$(flybase-version)"
wormbase="$(wormbase-version)"

cat << EOF
Ensembl: ${ensembl}
GENCODE: ${gencode}
RefSeq: ${refseq}
FlyBase: ${flybase}
WormBase: ${wormbase}
EOF
