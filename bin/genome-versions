#!/usr/bin/env bash
# shellcheck disable=SC2001
set -Eeu -o pipefail

# shellcheck source=/dev/null
source "$(koopa header bash)"

# Genome versions.
# Updated 2019-09-20.

ensembl="$(ensembl-version)"
gencode="$(gencode-version | sed 's/^/  - /')"
refseq="$(refseq-version)"
flybase="$(flybase-version | sed 's/^/  - /')"
wormbase="$(wormbase-version)"

cat << EOF
Ensembl: ${ensembl}
GENCODE:
${gencode}
RefSeq: ${refseq}
FlyBase:
${flybase}
WormBase: ${wormbase}
EOF
