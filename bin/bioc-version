#!/usr/bin/env bash
set -Eeu -o pipefail

# shellcheck source=/dev/null
source "$(koopa header bash)"

url="http://bioconductor.org/bioc-version"



# Usage                                                                     {{{1
# ==============================================================================

usage() {
cat << EOF
$(_koopa_help_header)

Bioconductor version.

optional arguments:
    --help, -h
        Show this help message and exit.

details:
    Extract the Bioconductor release version from bioconductor.org.

see also:
    ${url}

note:
    Bash script.
    Updated 2019-09-20.
EOF
}

_koopa_help "$@"



# Script                                                                    {{{1
# ==============================================================================

wget -q -O - "$url"
